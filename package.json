{
  "name": "gabstevens",
  "version": "1.0.0",
  "private": true,
  "description": "gabstevens",
  "author": "Gabriele Stefani",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --fix '**/*.+(js|jsx)'",
    "i18n": "yarn i18n:extract && yarn i18n:compile",
    "i18n:extract": "formatjs extract 'src/**/*.jsx' --out-file lang/en.json",
    "i18n:compile": "formatjs compile-folder lang src/i18n/translations",
    "prepare": "husky install"
  },
  "dependencies": {
    "gatsby": "^4.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1"
  }
}
